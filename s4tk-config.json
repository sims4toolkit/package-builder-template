{
  "buildFolders": [
    "./out"
  ],
  "buildName": "MyProject",
  "cachePath": "./s4tk-cache.json",
  "cancelOnError": true,
  "sourceFolder": "./src",
  "sourcePatterns": {
    "packages": [
      "./packages/**/*.package"
    ],
    "simdata": [
      "./xml/**/*.SimData.xml"
    ],
    "tuning": [
      "./xml/**/!(*.SimData).xml"
    ]
  }
}